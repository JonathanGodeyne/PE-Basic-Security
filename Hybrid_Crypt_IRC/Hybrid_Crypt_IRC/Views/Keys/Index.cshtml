@model Hybrid_Crypt_IRC.Models.ViewModels.PublicKeyViewModel
@{
    ViewData["Title"] = "Key management page";
}
<h1>@ViewData["Title"]</h1>

<div class="row">
<div class="col-md-4">
    <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.PublicKey)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.PublicKey)
        </dd>
    <hr/>
    <div class="row">
        <form class="form-group" asp-action="GenerateKeys" >
            <input type="submit" class="btn btn-primary" value="Generate Keys"/>
        </form>
        <p>&nbsp;&nbsp;</p>
        <form class="form-group" asp-action="UploadPrivateKey" style="margin-right: 5px">
            <input type="submit" class="btn btn-primary" value="Upload privatekey"/>
        </form>
    </div>
    
    <p
</div>
</div>